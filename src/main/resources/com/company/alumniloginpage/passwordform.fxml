<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.alumniloginpage.PasswordController">
    <Label text="Create Password" style="-fx-font-size: 16px; -fx-font-weight: bold;"/>
    <TextField fx:id="passwordField" promptText="Enter Password"/>
    <TextField fx:id="confirmPasswordField" promptText="Confirm Password"/>
    <Button text="Submit" onAction="#handleSubmitPassword"/>
</VBox>